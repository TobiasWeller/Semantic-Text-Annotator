	"SpecialPages": {
		"SemanticTextAnnotator": "SemanticTextAnnotatorSpecial"
	},
	"namespaces": [
		{
			"id": 248,
			"constant": "NS_ANNOTATION",
			"name": "Annotation"
		},
		{
			"id": 249,
			"constant": "NS_ANNOTATION_TALK",
			"name": "Annotation_talk"
		}
	],
	"callback": "SemanticTextAnnotatorHooks::afterInit",
	"manifest_version": 1
}
